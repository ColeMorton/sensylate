{
  "name": "cole-morton",
  "version": "5.5.1",
  "description": "Cole Morton - Personal website and blog",
  "author": "Cole Morton",
  "license": "MIT",
  "type": "module",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "dev": "NODE_ENV=development yarn generate-json && yarn generate-pages && astro dev",
    "build": "yarn validate-env && yarn generate-json && yarn generate-pages && astro build && yarn sanitize-build",
    "preview": "astro preview",
    "check": "astro check",
    "format": "prettier -w ./src",
    "generate-json": "node scripts/jsonGenerator.js",
    "generate-pages": "node scripts/pageGenerator.js",
    "remove-darkmode": "node scripts/removeDarkmode.js && yarn format",
    "validate-env": "node scripts/sanitize-build.js",
    "sanitize-build": "node scripts/sanitize-build.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "vitest run src/test/e2e",
    "test:e2e:watch": "vitest watch src/test/e2e",
    "test:e2e:ui": "E2E_SCREENSHOTS=true vitest run src/test/e2e",
    "lint": "eslint src --fix",
    "lint:check": "eslint src",
    "check-git-tracking": "node scripts/check-git-tracking.js",
    "pre-commit": "npm run lint:check && npm run check-git-tracking && npm run validate-env"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/mdx": "4.2.5",
    "@astrojs/react": "4.2.5",
    "@astrojs/rss": "4.0.11",
    "@astrojs/sitemap": "3.3.1",
    "@digi4care/astro-google-tagmanager": "^1.4.0",
    "@justinribeiro/lite-youtube": "^1.8.0",
    "@types/three": "^0.177.0",
    "astro": "5.7.8",
    "astro-auto-import": "^0.4.4",
    "astro-font": "^1.1.0",
    "date-fns": "^4.1.0",
    "disqus-react": "^1.1.6",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "marked": "^15.0.11",
    "plotly.js": "^3.0.1",
    "plotly.js-basic-dist": "^3.0.1",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-plotly.js": "^2.6.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "swiper": "^11.2.6",
    "three": "^0.177.0",
    "vite": "^6.3.3",
    "yet-another-react-lightbox": "^3.23.3"
  },
  "devDependencies": {
    "@astrojs/netlify": "^6.4.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "22.15.3",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@types/react-plotly.js": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.25.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "puppeteer": "^24.10.2",
    "sharp": "0.33.5",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
