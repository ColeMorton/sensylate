{
  "name": "cole-morton",
  "version": "5.5.1",
  "description": "Cole Morton - Personal website and blog",
  "author": "Cole Morton",
  "license": "MIT",
  "type": "module",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "dev": "NODE_ENV=development yarn generate-json && yarn generate-pages && astro dev",
    "build": "yarn validate-env && yarn generate-json && yarn generate-pages && astro build && yarn sanitize-build",
    "preview": "astro preview",
    "check": "astro check",
    "format": "prettier -w ./src",
    "generate-json": "node scripts/jsonGenerator.js",
    "generate-pages": "node scripts/pageGenerator.js",
    "remove-darkmode": "node scripts/removeDarkmode.js && yarn format",
    "validate-env": "node scripts/validate-env.js",
    "sanitize-build": "node scripts/sanitize-build.js",
    "test": "yarn test:unit",
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:unit:watch": "vitest watch --config vitest.unit.config.ts",
    "test:unit:fast": "vitest run src/test/components src/test/hooks src/test/featureFlags.test.ts --config vitest.unit.config.ts",
    "test:integration": "vitest run src/test/integration src/test/photo-booth/integration --config vitest.unit.config.ts",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:e2e:watch": "vitest watch --config vitest.e2e.config.ts",
    "test:e2e:ui": "E2E_SCREENSHOTS=true vitest run --config vitest.e2e.config.ts",
    "test:all": "yarn test:unit && yarn test:integration && yarn test:e2e",
    "test:coverage": "vitest run --coverage --config vitest.unit.config.ts",
    "test:photo-booth": "tsx src/test/photo-booth/photo-booth-test-suite.ts",
    "test:photo-booth:unit": "vitest run src/test/photo-booth/unit --config vitest.unit.config.ts",
    "test:photo-booth:integration": "vitest run src/test/photo-booth/integration --config vitest.unit.config.ts",
    "test:photo-booth:e2e": "vitest run src/test/photo-booth/e2e --config vitest.e2e.config.ts",
    "test:photo-booth:e2e:dev": "PHOTOBOOTH_E2E_DEV=true NODE_ENV=development vitest run src/test/photo-booth/e2e --config vitest.e2e.config.ts",
    "test:photo-booth:e2e:prod": "PHOTOBOOTH_E2E_DEV=false NODE_ENV=production vitest run src/test/photo-booth/e2e --config vitest.e2e.config.ts",
    "photo-booth:generate": "python3 ../scripts/photo_booth_generator.py",
    "photo-booth:generate:single": "python3 ../scripts/photo_booth_generator.py --dashboard",
    "photo-booth:generate:light": "python3 ../scripts/photo_booth_generator.py --mode light",
    "photo-booth:generate:dark": "python3 ../scripts/photo_booth_generator.py --mode dark",
    "photo-booth:export": "../scripts/export_with_server.sh",
    "photo-booth:export:portfolio": "../scripts/export_with_server.sh --dashboard portfolio_history_portrait --aspect-ratio 3:4",
    "photo-booth:export:fundamental": "../scripts/export_with_server.sh --dashboard fundamental_analysis --aspect-ratio 16:9",
    "photo-booth:cleanup": "python3 ../scripts/photo_booth_generator.py --cleanup",
    "flags:sync": "node scripts/sync-flags.js",
    "flags:validate": "node scripts/sync-flags.js validate",
    "flags:validate-quick": "node scripts/sync-flags.js validate-quick",
    "flags:generate-env": "node scripts/sync-flags.js generate-env",
    "flags:generate-netlify": "node scripts/sync-flags.js generate-netlify",
    "flags:add": "node scripts/sync-flags.js add",
    "flags:remove": "node scripts/sync-flags.js remove",
    "flags:list": "node scripts/sync-flags.js list",
    "flags:status": "node scripts/sync-flags.js status",
    "data:pipeline": "cd .. && python3 scripts/data_pipeline_manager.py",
    "data:pipeline:dry-run": "cd .. && python3 scripts/data_pipeline_manager.py --dry-run --format=json",
    "data:pipeline:dry-run:table": "cd .. && python3 scripts/data_pipeline_manager.py --dry-run --format=table",
    "lint": "eslint src --fix",
    "lint:check": "eslint src",
    "check-git-tracking": "node scripts/check-git-tracking.js",
    "pre-commit": "npm run lint:check && npm run check-git-tracking && npm run validate-env && npm run flags:validate-quick"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/mdx": "4.3.3",
    "@astrojs/react": "4.3.1",
    "@astrojs/rss": "4.0.12",
    "@astrojs/sitemap": "3.4.2",
    "@digi4care/astro-google-tagmanager": "^1.4.0",
    "@justinribeiro/lite-youtube": "^1.8.0",
    "@tabler/icons-react": "^3.27.0",
    "@types/three": "^0.177.0",
    "astro": "5.12.8",
    "astro-auto-import": "^0.4.4",
    "astro-font": "^1.1.0",
    "date-fns": "^4.1.0",
    "disqus-react": "^1.1.6",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "marked": "^15.0.11",
    "plotly.js": "^3.0.1",
    "plotly.js-basic-dist": "^3.0.1",
    "plotly.js-dist": "^3.0.1",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-plotly.js": "^2.6.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "swiper": "^11.2.6",
    "three": "^0.177.0",
    "vite": "^6.3.3",
    "yet-another-react-lightbox": "^3.23.3"
  },
  "devDependencies": {
    "@astrojs/netlify": "6.5.5",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "22.15.3",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@types/react-plotly.js": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.25.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "puppeteer": "^24.10.2",
    "sharp": "0.33.5",
    "tailwindcss": "^4.1.4",
    "tsx": "^4.19.5",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
