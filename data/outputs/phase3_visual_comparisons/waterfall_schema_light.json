{
  "engine": "plotly",
  "chart_type": "enhanced_waterfall",
  "version": "1.0.0",
  "theme": {
    "mode": "light",
    "colors": {
      "primary_data": "#26c6da",
      "secondary_data": "#7e57c2",
      "tertiary_data": "#3179f5",
      "background": "#fff",
      "text": "#121212",
      "borders": "#eaeaea"
    },
    "typography": {
      "font_family": "Heebo, Helvetica Neue, Arial, DejaVu Sans, Liberation Sans, sans-serif",
